{
  "name": "ocean-project",
  "version": "1.0.0",
  "description": "The next gen of eLearning",
  "repository": {
    "type": "git, module",
    "url": "git+https://github.com/GoogleCloudPlatform/nodejs-getting-started.git"
  },
  "private": true,
  "scripts": {
    "gulp": "gulp",
    "minify": "gulp minify",
    "start": "node app.js",
    "test": "repo-tools test app && ava -t 30s --tap test/*.test.js",
    "e2e": "repo-tools test deploy && E2E_TESTS=True ava -t 30s --tap test/*.test.js",
    "cover": "nyc --cache npm test; nyc report --reporter=html"
  },
  "author": "Shaul Mazor",
  "license": "Apache-2.0",
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "dependencies": {
    "@google-cloud/datastore": "1.3.4",
    "@google-cloud/storage": "1.6.0",
    "body-parser": "1.18.2",
    "express": "4.16.2",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.5",
    "module-alias": "2.1.0",
    "nconf": "0.10.0",
    "prompt": "1.0.0"
  },
  "devDependencies": {
    "browser-sync": "^2.26.13",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-sass": "^4.1.0",
    "node-sass": "^5.0.0"
  },
  "engines": {
    "node": "8.x.x"
  },
  "_moduleAliases": {
    "@auth-service": "."
  },
  "bugs": {
    "url": "https://github.com/GoogleCloudPlatform/nodejs-getting-started/issues"
  },
  "homepage": "https://github.com/GoogleCloudPlatform/nodejs-getting-started#readme",
  "main": "app.js"
}
